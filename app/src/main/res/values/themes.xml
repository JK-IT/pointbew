<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.WebPoint" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#D16014</item>
        <item name="colorPrimaryVariant">#FF924A</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#1BBCD1</item>
        <item name="colorSecondaryVariant">#007585</item>
        <item name="colorOnSecondary">#FFFFFF</item>

        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimary</item>
        <!-- Customize your theme here. -->

        <!-- set this so the view will no overlap the action bar -->
        <item name="windowActionModeOverlay">true</item>
        <item name="actionModeBackground">@color/action_mode_overlay</item>

        <!-- Dialog Theme -->
        <item name="materialAlertDialogTheme">@style/KusDialogTheme</item>
        <item name="floatingActionButtonStyle">?attr/colorPrimaryVariant</item>

        <!-- font family -->
        <item name="android:fontFamily">@font/montserratregular</item>
        <item name="android:typeface">sans</item>


    </style>



</resources>